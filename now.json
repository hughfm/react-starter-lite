{
  "name": "react-starter-lite",
  "version": 2,
  "alias": [],
  "scope": "my@email.here",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    { "src": "index.html", "use": "@now/static" },
    { "src": "src/main.css", "use": "@now/static" }
  ],
  "routes": [
    { "src": "^/build/(.*).js", "dest": "$1.js" }
  ]
}
